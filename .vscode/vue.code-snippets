  "Quickly creates an empty add page": {
    "prefix": "ozelot:create",
    "body": [
      "<template>",
      "  <div class=\"card card-flush datatable\">",
      "",
      "    <div class=\"card-header\">",
      "      <h2 class=\"card-title\">Add ${1:todo}</h2>",
      "      <div class=\"card-toolbar\">",
      "        <router-link to=\"index\" class=\"btn btn-flex btn-light\">Return</router-link>",
      "        <button @click.prevent=\"create()\" class=\"btn btn-flex btn-primary\">Add</button>",
      "      </div>",
      "    </div>",
      "",
      "    <div class=\"card-body\">",
      "      <form class=\"row mb-4 g-9\">",
      "        <input type=\"hidden\" v-model=\"${1:todo}.id\" />",
      "        <div class=\"col-sm-6\">",
      "          <label class=\"required form-label\">Title</label>",
      "          <input type=\"text\" v-model=\"${1:todo}.title1\" class=\"form-control form-control-solid\" placeholder=\"Placeholder\" />",
      "          <span class=\"invalid-feedback\"> {{ v$.${1:todo}.title1.$$errors[0]?.$$message }} </span>",
      "        </div>",
      "        <div class=\"col-sm-6\">",
      "          <label class=\"required form-label\">Title</label>",
      "          <input type=\"text\" v-model=\"${1:todo}.title2\" class=\"form-control form-control-solid\" placeholder=\"Placeholder\" />",
      "          <span class=\"invalid-feedback\"> {{ v$.${1:todo}.title2.$$errors[0]?.$$message }} </span>",
      "        </div>",
      "      </form>",
      "    </div>",
      "",
      "  </div>",
      "</template>",
      "",
      "<script>",
      "import { useVuelidate } from '@vuelidate/core'",
      "import { required } from '@vuelidate/validators'",
      "",
      "export default {",
      "  setup: () => ({ v$: useVuelidate() }),",
      "  data() {",
      "    return {",
      "      ${1:todo}: {",
      "        id: 0,",
      "        title1: \"\",",
      "        title2: \"\"",
      "      },",
      "    }",
      "  },",
      "  validations() {",
      "    return {",
      "      ${1:todo}: {",
      "        title1: { required },",
      "        title2: { required },",
      "      },",
      "    }",
      "  },",
      "  methods: {",
      "    async create() {",
      "      const valid = await this.v$.$$validate()",
      "      if (!valid) return",
      "      const block = new KTBlockUI(this.$$el)",
      "      const result = await axios.post(\"${1:todo}s\", this.${1:todo})",
      "      await swal.fire(\"Success\", \"${1:todo} added successfully\", \"success\")",
      "      block.releaseDestroy()",
      "      this.$$router.push(\"index\")",
      "    }",
      "  }",
      "}",
      "</script>"
    ],
    "description": "Quickly creates an empty add page",
    "scope": "vue"
  },
  "Quickly creates an empty edit page": {
    "prefix": "ozelot:edit",
    "body": [
      "<template>",
      "  <div class=\"card card-flush datatable\">",
      "",
      "    <div class=\"card-header\">",
      "      <h2 class=\"card-title\">Edit ${1:todo}</h2>",
      "      <div class=\"card-toolbar\">",
      "        <router-link to=\"..index\" class=\"btn btn-flex btn-light\">Return</router-link>",
      "        <button @click.prevent=\"edit()\" class=\"btn btn-flex btn-primary\">Edit</button>",
      "      </div>",
      "    </div>",
      "",
      "    <div class=\"card-body\">",
      "      <form class=\"row mb-4 g-9\">",
      "        <input type=\"hidden\" v-model=\"${1:todo}.id\" />",
      "        <div class=\"col-sm-6\">",
      "          <label class=\"required form-label\">Title</label>",
      "          <input type=\"text\" v-model=\"${1:todo}.title1\" class=\"form-control form-control-solid\" placeholder=\"Placeholder\" />",
      "          <span class=\"invalid-feedback\"> {{ v$.${1:todo}.title1.$$errors[0]?.$$message }} </span>",
      "        </div>",
      "        <div class=\"col-sm-6\">",
      "          <label class=\"required form-label\">Title</label>",
      "          <input type=\"text\" v-model=\"${1:todo}.title2\" class=\"form-control form-control-solid\" placeholder=\"Placeholder\" />",
      "          <span class=\"invalid-feedback\"> {{ v$.${1:todo}.title2.$$errors[0]?.$$message }} </span>",
      "        </div>",
      "      </form>",
      "    </div>",
      "",
      "  </div>",
      "</template>",
      "",
      "<script>",
      "import { useVuelidate } from '@vuelidate/core'",
      "import { required } from '@vuelidate/validators'",
      "",
      "export default {",
      "  setup: () => ({ v$: useVuelidate() }),",
      "  data() {",
      "    return {",
      "      ${1:todo}: {",
      "        id: 0,",
      "        title1: \"\",",
      "        title2: \"\"",
      "      },",
      "    }",
      "  },",
      "  validations() {",
      "    return {",
      "      ${1:todo}: {",
      "        title1: { required },",
      "        title2: { required },",
      "      },",
      "    }",
      "  },",
      "  async beforeRouteEnter(to, from, next) {",
      "    const result = await axios.get(`${1:todo}s/${to.params.id}`)",
      "    next(vm => { vm.${1:todo} = result.data })",
      "  },",
      "  methods: {",
      "    async edit() {",
      "      const valid = await this.v$.$$validate()",
      "      if (!valid) return",
      "      const block = new KTBlockUI(this.$$el)",
      "      const result = await axios.put(`${1:todo}s/${this.${1:todo}.id}`, this.${1:todo})",
      "      await swal.fire(\"Success\", \"${1:todo} edited successfully\", \"success\")",
      "      block.releaseDestroy()",
      "      this.$$router.push(\"../index\")",
      "    }",
      "  }",
      "}",
      "</script>"
    ],
    "description": "Quickly creates an empty edit page",
    "scope": "vue"
